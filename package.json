{
  "name": "lightbulb",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "run-p serve:graphql serve:dev",
    "serve:dev": "node src/server.js",
    "lint": "eslint src/**/*.tsx",
    "migrate": "db-migrate up --env=dev",
    "docker:migrate": "db-migrate up --env=docker",
    "dev:db:reset": "db-migrate down --env=dev && db-migrate up --dev=env",
    "serve:graphql": "graphql-codegen --config codegen.yml -w",
    "build:graphql": "graphql-codegen --config codegen.yml",
    "build:postgraphile": "postgraphile -c http://postgres:postgres@postgres:5432/postgres -X --export-schema-graphql=./src/graphql/schema.graphql"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && pretty-quick --staged"
    }
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cypress/"
    ],
    "modulePathIgnorePatterns": [
      "/node_modules/",
      "/cypress/"
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@apollo/react-testing": "^3.1.3",
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.4",
    "@graphql-codegen/cli": "^1.9.1",
    "@graphql-codegen/typescript": "^1.9.1",
    "@graphql-codegen/typescript-graphql-files-modules": "^1.9.1",
    "@graphql-codegen/typescript-operations": "^1.9.1",
    "@graphql-codegen/typescript-react-apollo": "^1.9.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.12",
    "@types/react": "^16.9.13",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "apollo-link-schema": "^1.2.4",
    "babel-jest": "^24.9.0",
    "babel-plugin-inline-react-svg": "^1.1.0",
    "cypress": "^3.7.0",
    "db-migrate": "^0.11.6",
    "db-migrate-pg": "^1.0.0",
    "eslint": "^6.7.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.6",
    "husky": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-scss": "^2.0.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "tailwindcss": "^1.1.3",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "apollo-cache": "^1.3.2",
    "apollo-link": "^1.2.13",
    "apollo-utilities": "^1.3.2",
    "@zeit/next-css": "^1.0.1",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "^1.5.16",
    "formik": "^2.0.6",
    "graphql-import": "^0.7.1",
    "next": "^9.1.4",
    "node-fetch": "^2.6.0",
    "polka": "^0.5.2",
    "postgraphile": "^4.5.0",
    "react": "^16.12.0",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.12.0"
  }
}
